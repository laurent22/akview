{
	"title": "JPEG auto-rotation",
	"description": "Rotate JPEG according to Exif orientation tag",
	"dependencies": [
		{ "command": "jhead" },
		{ "command": "jpegtran", "package": "libjpeg" }
	],
	"win_dependencies": [
		{ "command": "jhead" },
		{ "command": "jpegtran" }
	],
	"command": [ "jhead", "-autorot", "__FILE__" ],
	"shortcuts": [ "Ctrl + Shift + O" ],
	"menu": "transform"
}